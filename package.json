    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.0.3",
    "protractor": "~7.0.0",
    "ts-essentials": "^7.0.0",
    "ts-node": "~8.10.2",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.3",
    "typescript": "3.9.7",
